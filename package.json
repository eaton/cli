{
  "name": "@eatonfyi/cli",
  "version": "0.1.0",
  "description": "A collection of handy CLI utilities, and consolidated wrappers to make them play nicely together.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist",
    "README.md"
  ],
  "scripts": {
    "build": "pkgroll",
    "demo": "node --experimental-strip-types ./demo.mjs",
    "package": "pkgroll --clean-dist --minify",
    "test": "tap"
  },
  "keywords": [
    "cli",
    "terminal"
  ],
  "author": "Jeff Eaton <jeff@eaton.fyi> (https://eaton.fyi)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eaton/cli.git"
  },
  "devDependencies": {
    "@tapjs/typescript": "^3.0.0",
    "@types/node": "^22.8.6",
    "pkgroll": "^2.5.1",
    "tap": "^21.0.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "ansi-escapes": "^7.0.0",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "configstore": "^7.0.0",
    "date-fns": "^4.1.0",
    "emittery": "^1.0.3",
    "exit-hook": "^4.0.0",
    "log-update": "^6.1.0",
    "open": "^10.1.0",
    "restore-cursor": "^5.1.0",
    "sparkly": "^6.0.0",
    "tasuku": "^2.0.1",
    "terminal-size": "^4.0.0"
  }
}
